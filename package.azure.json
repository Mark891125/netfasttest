{
  "name": "netfasttest",
  "version": "0.1.0",
  "private": true,
  "description": "网络速度测试应用程序 - 支持IP地理位置分析和网络性能测试",
  "keywords": ["network", "speed-test", "azure", "nextjs", "ip-geolocation"],
  "author": "Network Speed Test Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "azure:build": "npm ci --omit=dev && npm run build",
    "azure:start": "next start -p ${PORT:-8080}",
    "azure:dev": "next dev -p ${PORT:-3000}",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rm -rf .next node_modules/.cache",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "next": "15.3.4",
    "date-fns": "^4.1.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "@eslint/eslintrc": "^3"
  },
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/netfasttest.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/netfasttest/issues"
  },
  "homepage": "https://github.com/yourusername/netfasttest#readme"
}
