datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model TestResult {
    id            String    @id
    storeID       String
    clientTime    DateTime?
    receptionTime DateTime?
    returnTime    DateTime?
    delay         Int
    ip            String
    location      String? // 可选字段
}

model Store {
    id      String @id
    code    String @unique
    name    String
    address String

    latitude  Float?
    longitude Float?
}
